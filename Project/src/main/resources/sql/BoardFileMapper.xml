<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="example.dao.BoardFileDao">
  <resultMap type="boardFile" id="boardFileMap">
    <id column="ipno" property="boardPhotoNo"/> 
    <result column="tmno" property="travelMainNo"/>
    <result column="ipnm" property="filename"/>
  </resultMap>
  
  <insert id="insert" parameterType="boardFile"  >
    insert into tvl_intro_pho(tmno, ipnm)
    values(#{travelMainNo}, #{fileName})
  </insert>
</mapper>




















